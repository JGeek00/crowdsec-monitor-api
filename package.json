{
  "name": "crowdsec-monitor-api",
  "version": "1.3.0",
  "description": "REST API server for CrowdSec LAPI",
  "license": "MIT",
  "author": "JGeek00",
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "build:dev": "tsc",
    "build:prod": "npm run build:prod:compile && npm run build:prod:obfuscate",
    "build:prod:compile": "tsc --sourceMap false --declaration false --declarationMap false",
    "build:prod:obfuscate": "node scripts/obfuscate.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^25.2.3",
    "javascript-obfuscator": "^5.3.0",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
